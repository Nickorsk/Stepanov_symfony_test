<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="login">
    {{ form_row(form.username, { 'label' : 'Логин' }) }}
    {{ form_row(form.email, { 'label' : 'Email' }) }}
    {{ form_row(form.plainPassword.first, { 'label' : 'Пароль' }) }}
    {{ form_row(form.plainPassword.second, { 'label' : 'Повторите пароль' }) }}
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="Регистрация" />
    </div>
</form>
